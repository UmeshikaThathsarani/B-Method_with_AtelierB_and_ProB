MACHINE  PaperRound

  VARIABLES 
    houseset

  INVARIANT 
    houseset <: NAT1 

  INITIALISATION 
    houseset := {}

  OPERATIONS

    add(new) =
		PRE  new : NAT1  &  new /: houseset
		THEN houseset := houseset \/ { new }
    END;

    ans <-- number =
        BEGIN
			ans := card( houseset )
        END;
		
	ans1 <-- getspapers(hn) =
        PRE hn : NAT1 & hn : houseset
            THEN
                ans1 := 1
            END ;
        
	delete(hn) =
        PRE hn : NAT1 & hn : houseset
            THEN
                houseset := houseset - {hn}
            END

END
































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































